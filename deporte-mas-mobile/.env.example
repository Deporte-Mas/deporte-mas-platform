# Mux Configuration
# Get your environment key from: https://dashboard.mux.com/settings/data
EXPO_PUBLIC_MUX_ENV_KEY=your_mux_environment_key_here

# Default Mux Playback ID for demo streams
# Get from: https://dashboard.mux.com/ (Video or Live Streams section)
EXPO_PUBLIC_MUX_PLAYBACK_ID=your_mux_playback_id_here
# Subscription Configuration
# URL where users will be redirected for subscription purchases
# This must be an HTTPS URL
EXPO_PUBLIC_SUBSCRIPTION_URL=https://deporte-mas-platform.vercel.app/

# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-publishable-key-here
